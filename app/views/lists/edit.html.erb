<div class="listeditPage">
  <div class="container">
    <%= form_with model: @list, class: 'new_list', local: true do |f| %>
      <% if @list.errors.any? %>
        <p class="text-danger">タイトルは1-255文字以内</p>
      <% end %>
      <%= f.label :title %>
      <%= f.text_field :title, class: 'form-control listName', placeholder: 'リスト名' %>
      <div class="text-center"><%= f.submit '編集', class: 'submitBtn' %></div>
    <% end %>
  </div>
</div>